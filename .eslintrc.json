{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "@typescript-eslint/no-unused-vars": "error",
    "prettier/prettier": ["error", {
      "endOfLine": "auto",
      "tabWidth": 2,
      "semi": true,
      "singleQuote": true
    }]
  }
}
